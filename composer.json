{
  "name": "ad-mos/laravel-datatables",
  "description": "DataTables Server-Side API for Laravel Framework",
  "keywords": [
    "laravel",
    "dataTables"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Mospan",
      "email": "adammospan@gmail.com"
    },
    {
      "name": "Yaroslav Romanenko",
      "email": "yaroslav.romanenko@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "doctrine/dbal": "^2.6|^3.0",
    "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10",
    "illuminate/http": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10"
  },
  "autoload": {
    "psr-4": {
      "AdMos\\DataTables\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdMos\\DataTables\\DataTablesServiceProvider"
      ],
      "aliases": {
        "DataTables": "AdMos\\DataTables\\Facades\\DataTables"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
